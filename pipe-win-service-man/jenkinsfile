pipeline {
  agent any
  stages {
    stage('PowerShell Version') {
      steps {
        sh 'pwsh --version'
      }
    }
    stage('Drive Size') {
      steps {
        sh 'df -h | grep sda'
        sh 'pwsh -command "Get-PSDrive | where name -match /"'
      }
    }
    stage('Get Process') {
      steps {
        sh 'pwsh pipe-win-service-man/get-process.ps1'
      }
    }
  }
}